{
    "name": "ajimoti/modelplus",
    "description": "An intuitive UI for browsing Laravel Eloquent models with smart relationship handling",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "browser",
        "admin",
        "ui"
    ],
    "authors": [
        {
            "name": "John Ajimoti",
            "email": "johnajimoti@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/contracts": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/routing": "^11.0",
        "illuminate/view": "^11.0",
        "symfony/finder": "^7.0.3"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ajimoti\\ModelPlus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ajimoti\\ModelPlus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ajimoti\\ModelPlus\\ModelPlusServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
} 